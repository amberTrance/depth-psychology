<!DOCTYPE html>
<html lang="en">
<!-- head -->
<%- include('../partials/head.ejs') %>
<body>
    <div class="container">
        <!-- navbar -->
        <%- include('../partials/nav.ejs') %>

        <div class="part">
            <div class="text">
                <h1><%= reqArchetype[0].archetype %></h1>
                <% reqArchetype.forEach(arc => { %>
                    <p><%= arc.text %></p>
                    <small><b>&#9830 <%= arc.author %></b>, <%= arc.source %></small><br><br>
                <% }) %>
            </div>
            
            <%- include('./partials/archNav.ejs') %>
        </div>
    </div>
    <%- include('../partials/footer.ejs') %>
</body>
</html>