<div class="symbols">
    <img src="/images/mandala.jpg" alt="" class="smallMandala">
    <a href="/symbol/create" class="add-btn">Add New Symbol</a>
    <h3>Categories :</h3>
    <ul>
        <% symbols.forEach(symbol => { %>
            <li>
                <a href="/symbols/<%= symbol %>"><%= symbol %></a>
            </li>
        <% }) %>
    </ul>
</div>