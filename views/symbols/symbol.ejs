<!DOCTYPE html>
<html lang="en">
<!-- head -->
<%- include('../partials/head.ejs') %>
<body>
    <div class="container">
        <!-- navbar -->
        <%- include('../partials/nav.ejs') %>

        <div class="part">
            <div class="text">
                <h1><%= reqSymbol[0].symbol %></h1>

                <% reqSymbol.forEach(symbol => { %>
                    <div class="group">
                        <p><%= symbol.text %></p>
                        <small><b>&#9830 <%= symbol.author %></b>, <%= symbol.source %></small>
                    </div>

                <% }) %>
            </div>
           
            <%- include('../partials/symbolsNav.ejs') %>
        </div>        
        
    </div>
    <%- include('../partials/footer.ejs') %>
</body>
</html>
